Renter.controller('NewRateCtrl', ['$scope', '$http', function($scope, $http, $state){

  var url = "<%= ENV['URL'] %>" || "http://localhost:3000/api/";

  $scope.agent_rating = {};
  $scope.form = {};

  $scope.form.create = function(){
    $http({
      url: url + 'agent_ratings',
      method: 'POST',
      data: $scope.agent_rating
    }).then(function(resp){
      $scope.agent_rating = ''
    }, function(resp) {
      console.log(resp);
    })
  };
}]);