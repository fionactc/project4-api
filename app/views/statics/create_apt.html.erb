<div class="container">
  <div class="row">
    <h1>Create A New agent_rating</h1>

    <form ng-submit="form.create()" ng-controller="NewRateCtrl" ng-app="RenterApp">
      <div class="form-group">
        <label>Title</label>
        <input type="text" class="form-control" placeholder="Enter agent_rating" ng-model="agent_rating.ar_overall_star">
      </div>

      <div class="form-group">
        <label>Content</label>
        <input type="text" class="form-control" placeholder="Enter comment" ng-model="agent_rating.ar_comment">
      </div>

      <button type="submit" class="btn btn-default">Submit</button>
    </form>
  </div>
</div>